{
  "name": "tess-extrude",
  "version": "0.1.0",
  "description": "Extrude 2D polygon outlines into THREE.BufferGeometry using poly2tri Delaunay tessellation",
  "type": "module",
  "main": "dist/tess-extrude.cjs.js",
  "module": "dist/tess-extrude.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/tess-extrude.es.js",
      "require": "./dist/tess-extrude.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "build:demo": "vite build --config vite.config.demo.ts",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "three",
    "threejs",
    "3d",
    "extrude",
    "extrusion",
    "polygon",
    "tessellation",
    "poly2tri",
    "geometry",
    "webgl",
    "svg"
  ],
  "author": "Alexandros Karatzaferis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ekaratzaferis/tess-extrude.git"
  },
  "bugs": {
    "url": "https://github.com/ekaratzaferis/tess-extrude/issues"
  },
  "homepage": "https://github.com/ekaratzaferis/tess-extrude#readme",
  "peerDependencies": {
    "three": ">=0.150.0"
  },
  "dependencies": {
    "poly2tri": "^1.5.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0",
    "three": "^0.170.0",
    "@types/three": "^0.170.0",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.9.0",
    "vitest": "^1.6.0"
  }
}
